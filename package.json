{
  "name": "dentemple.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:update": "react-scripts test --env=jsdom --updateSnapshot",
    "test:coverage": "react-scripts test --env=jsdom --coverage"
  },
  "dependencies": {
    "material-ui": "^0.19.4",
    "react": "^16.0.0",
    "react-animations": "^0.1.0",
    "react-aux": "^1.0.1",
    "react-code-splitting": "^1.2.1",
    "react-dom": "^16.0.0",
    "react-icons": "^2.2.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "styled-components": "^2.2.1",
    "typeface-exo": "^0.0.35",
    "typeface-roboto": "^0.0.35"
  },
  "devDependencies": {
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "husky": "^0.14.3",
    "jest-enzyme": "^4.0.1",
    "lint-staged": "^4.3.0",
    "prettier": "^1.7.4",
    "react-test-renderer": "^16.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --single-quote --jsx-bracket-same-line --write",
      "git add"
    ]
  }
}
