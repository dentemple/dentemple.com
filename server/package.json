{
  "name": "dentemple-server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "license": "MIT",
  "main": "dist",
  "scripts": {
    "--- APP ---": "",
    "start": "concurrently \"npm run build -- --watch\" \"npm run start:dev\"",
    "start:dev": "nodemon -q dist/index.js",
    "start:production": "npm run build && node dist",
    "build": "npx tsc",
    "--- CLEANUP ---": "",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "check:format": "npm run prettier -- --list-different",
    "check:types": "tsc --project tsconfig.json --pretty --noEmit"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.8.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/express": "^4.17.15",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.17",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "superagent": "^8.0.6",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4"
  }
}
